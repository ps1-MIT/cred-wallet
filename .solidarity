{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": "14.8.0"
      }
    ],
    "Watchman": [
      {
        "rule": "cli",
        "binary": "watchman",
        "error": "Please install watchman on this machine. Refer to the official Watchman installation instructions for additional help.",
        "platform": [
          "darwin",
          "linux"
        ]
      }
    ],
    "Package JSON": [
      {
        "rule": "file",
        "location": "./package.json"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": "6.14.7"
      }
    ],
    "Yarn": [
      {
        "rule": "cli",
        "binary": "yarn",
        "version": "--version",
        "semver": "1.22.4"
      }
    ],
    "CocoaPods": [
      {
        "rule": "cli",
        "binary": "pod",
        "platform": "darwin",
        "semver": "1.9.1"
      },
      {
        "rule": "dir",
        "location": "./ios/Pods"
      }
    ],
    "Xcode": [
      {
        "rule": "cli",
        "binary": "xcodebuild",
        "semver": "11.6",
        "platform": "darwin"
      },
      {
        "rule": "cli",
        "binary": "xcrun",
        "semver": "50",
        "platform": "darwin"
      }
    ]
  }
}